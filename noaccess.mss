@no-access: white;
@opacity-no-access: 0.6;

@paid-access: white;
@opacity-paid-access: 0.3;

#no_access {
   polygon-fill: @no-access;
   polygon-opacity: @opacity-no-access;
   polygon-pattern-file: url("symbols/no_access.png");
}

#paid_access {
   polygon-fill: @paid-access;
   polygon-opacity: @opacity-paid-access;
   polygon-pattern-file: url("symbols/no_access.png");
}
